<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Web Tutorial" %></title>
    
    <link href="stylesheets/normalize.css" rel="stylesheet" type="text/css" relative="true" /><link href="stylesheets/monokai_sublime.css" rel="stylesheet" type="text/css" relative="true" /><link href="stylesheets/ruby.css" rel="stylesheet" type="text/css" relative="true" /><link href="stylesheets/layout.css" rel="stylesheet" type="text/css" relative="true" />
    <script src="javascripts/highlight.js" type="text/javascript" relative="true"></script>
    <script>hljs.initHighlightingOnLoad();</script>
  </head>
  
  <body class="content">
    <%= partial "/layouts/partials/navbar" %>
    <%= yield %>
  </body>
</html>
